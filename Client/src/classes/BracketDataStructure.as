package classes{	public class BracketDataStructure{		public var leafData:String;		public var leafVisited:Boolean;//		public var predictedRight:Boolean;			public function BracketDataStructure(){			leafData = "";			leafVisited = false;//			predictedRight = false;		}				public function setLeafData(teamName:String):void{			leafData = teamName;		}				public function setLeafVisited(truthValue:Boolean):void{			leafVisited = truthValue;		}				public function getLeafData():String{			return leafData;		}				public function getLeafVisited():Boolean{			return leafVisited;		}	}}